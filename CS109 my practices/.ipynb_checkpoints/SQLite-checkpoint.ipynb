   "source": [
    "from sqlite3 import dbapi2 as sq3\n",
    "\n",
    "def get_db(dbfile):\n",
    "    sqlite_db = sq3.connect(dbfile)\n",
    "    return sqlite_db\n",
    "\n",
    "def init_db(dbfile,schema):\n",
    "    db = get_db(dbfile)\n",
    "    db.cursor().executescript(schema)\n",
    "    db.commit()\n",
    "    return db"
   ]
  }
